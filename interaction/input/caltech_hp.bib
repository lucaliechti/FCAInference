	Journal = {IEEE TRANSACTIONS ON AUTOMATIC CONTROL},
	Month = {Dec},
	Number = {12},
	Pages = {2297--2306},
	Publication-Type = {J},
	Title = {Lumpable hidden Markov models - Model reduction and reduced complexity filtering},
	Volume = {45},
	Year = {2000},
